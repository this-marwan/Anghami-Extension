{
"name" : "Anghami",
"description" : "get links to Anghami",

"version" : "1.2",
"manifest_version": 2,
"permissions":[ "activeTab", "tabs" , "*://*.youtube.com/*", "*://*.anghami.com/*"],

"icons":{
  "16":"icon.png",
  "48":"icon.png"
},

"browser_action":{
"default_icon": "icon.png",
"default_title" : "Get Link",
"default_popup" : "popup.html"

},

"content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["content.js"]
    }
  ],

"background":{
    "scripts": ["background.js"]
  }

}
